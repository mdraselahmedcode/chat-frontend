    "@react-navigation/native": "^6.1.7",
    "@reduxjs/toolkit": "^2.2.7",
    "axios": "^1.7.7",
    "date-fns": "^4.1.0",
    "expo": "^51.0.38",
    "expo-av": "~14.0.7",
    "expo-camera": "~15.0.16",
    "expo-clipboard": "~6.0.3",
